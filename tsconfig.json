{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["es6"],
    "outDir": "./lib",
    "rootDir": "./",
    "strict": false,
    "strictPropertyInitialization": false,
    "baseUrl": "./",
    "paths": {
      "@src/*": ["src/*"],
    },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}
